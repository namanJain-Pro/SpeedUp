<StackPane alignment="center" prefHeight="1000.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Scene.PracticeScreen.PracticeScreenController">
   <children>
      <AnchorPane prefHeight="1000.0" prefWidth="1500.0" style="-fx-background-color: #333333;">
         <children>
            <AnchorPane prefHeight="100.0" prefWidth="1500.0" style="-fx-background-color: #BAD1CD;">
               <children>
                  <Button fx:id="backButton" layoutX="1376.0" layoutY="25.0" mnemonicParsing="false" onAction="#goToHomeScreen" prefHeight="50.0" prefWidth="91.0" text="Back" textFill="#052f5f">
                     <font>
                        <Font name="System Bold Italic" size="23.0" />
                     </font>
                  </Button>
                  <Label layoutX="64.0" layoutY="25.0" prefHeight="39.0" prefWidth="84.0" text="Speed : " textFill="#052f5f">
                     <font>
                        <Font name="Berlin Sans FB Demi Bold" size="23.0" />
                     </font>
                  </Label>
                  <Label fx:id="wpmTab" layoutX="148.0" layoutY="15.0" prefHeight="60.0" prefWidth="66.0" text="0" textAlignment="CENTER">
                     <font>
                        <Font name="Bauhaus 93" size="40.0" />
                     </font>
                  </Label>
                  <Label layoutX="248.0" layoutY="25.0" prefHeight="39.0" prefWidth="72.0" text="Error :" textFill="#052f5f">
